   -
     name : RefreshList
     internal_properties : 
     procedure_id : 1409353274351335491
     type_code : 14
     code : |1-
      // Summary: Refreshes the list of downloads
      // Syntax:
      // RefreshList ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      procedure RefreshList()
      
      stDownload		is DownloadInfo
      nSpaceUsed		is numeric
      nTotalSpace		is numeric
      
      // Clears the current list
      LOOP_DOWNLOADS.DeleteAll()
      
      // Lists downloads still present
      for each stDownload of DownloadList()
      	
      	// Adds the download to the displayed list
      	nSpaceUsed += ShowDownload( stDownload )
      end
      
      // Calculates the total available space
      for i = 1 _to_ SysNbExternalStorage()
      	
      	// External storage space
      	nTotalSpace += SysMemoryExternalStorage(i, sysStorageSpace)
      end
      
      // Internal storage space
      if InSimulatorMode() then
      	nTotalSpace += SysSpace(sysProgramSpace)				
      else
      	nTotalSpace += SysSpace(sysStorageSpace)		
      end
      
      // Completes the information
      STC_TOTALSIZE_INFO	= "Using " + LengthToString(Max(0, nSpaceUsed)) + " on " + LengthToString(nTotalSpace)
      
      STC_EMPTY.Visible	= ( LOOP_DOWNLOADS.Count = 0 )
     type : 458752
   -
     name : DownloadElement
     procedure_id : 1409353274351401027
     type_code : 14
     code : |1-
      // Summary: Download the requested URL
      // Syntax:
      //DownloadElement (<sURL> is string)
      //
      // Parameters:
      //	sURL (UNICODE string): URL of the element
      // Return value:
      // 	None
      //
      procedure DownloadElement( local sURL is string )
      
      stDownload			is DownloadParameter
      stInfo				is DownloadInfo 
      
      if InSimulatorMode() then
      	Error("Unable to add downloads in simulator mode")
      	return
      end
      
      // Quickly checks the validity of the URL
      if sURL <> "" _and_ ( sURL [= "https://" _or_ sURL [= "http://" ) then
      	
      	// Create the download
      	stDownload.NotificationMode	= downloadNotifProgressAndCompletion
      	stDownload.Source			= sURL
      	
      	// Adds the download
      	let ID = DownloadAdd(stDownload)
      	
      	// Retrieves the information of the added download
      	stInfo = DownloadGetInfo(ID)
      	
      	// Shows the download
      	ShowDownload( stInfo, True )
      	
      end
     type : 458752
   -
     name : ShowDownload
     procedure_id : 1409353274351466563
     type_code : 14
     code : |1-
      // Summary: Shows a download in the list
      // Syntax:
      //[ <Result> = ] ShowDownload (<stDownload> is DownloadInfo [, <bFirst> is boolean [, <bToEdit> is boolean]])
      //
      // Parameters:
      //	stDownload (DownloadInfo): Details of the download
      //	bFirst (boolean - default value=0): True to show the download at the top of the list, False otherwise
      //	bToEdit (boolean - default value=0): True if an existing download must be updated, False otherwise
      // Return value:
      // 	numeric: Number of bytes occupied by the download
      //
      procedure ShowDownload( stDownload is DownloadInfo, bFirst is boolean = False, bToEdit is boolean = False ) : numeric
      
      N is int
      
      // Ignores invalid downloads
      if not stDownload.FileSize > 0 then
      	
      	if bFirst then
      		
      		// Refreshes the list
      		RefreshList()		
      		return 0
      	end
      end
      
      if bToEdit = False then
      	
      	// If you want the download to appear at the top of the list
      	if bFirst then
      		
      		N = 1
      		LOOP_DOWNLOADS.InsertLine(N, stDownload.Identifier, False)		
      		
      	else
      		
      		N = LOOP_DOWNLOADS.AddLine(stDownload.Identifier, False)		
      	end
      else
      	
      	// Searches for the download
      	N = LooperSearch(ATT_IDENTIFIER, stDownload.Identifier, searchExactMatch)
      	if N not > 0 then
      		return 0
      	end
      	
      	LOOP_DOWNLOADS[ N ].ATT_ERROR 	= False
      end
      
      // Gets the source info 
      sTypeInfo	is string	= URIGetInfo(stDownload.Destination, uriInfoOriginType) 
      
      // Extracts the MIME type
      sMIMEType	is string	= sTypeInfo.ExtractString(1)
      
      // Size and status
      sSizeInfo	is string
      sStatus		is string
      
      switch stDownload.Status
      
      	// The download has not started
      	case downloadStatusPending 
      		
      		// 0 of <Total size>
      		sSizeInfo								= "0 of " + LengthToString(stDownload.FileSize) + " • " + stDownload.Source.Host
      		sStatus									= "Starting..."
      		LOOP_DOWNLOADS[ N ].STC_FILE_INFO.Color	= DarkGray
      		
      		// Checks progress regularly
      		CheckDownloadRegularly( stDownload.Identifier, downloadStatusPending )
      		
      	// The download is in progress
      	case downloadStatusRunning  
      		
      		X is numeric = stDownload.Progress
      		if X <= 0 then X = 0
      		
      		// <Progress> of <Total size>
      		sSizeInfo	= LengthToString(X) + " on " + LengthToString(stDownload.FileSize) + " • " + stDownload.Source.Host
      		sStatus		= "In progress..."
      		
      		// Checks progress regularly
      		CheckDownloadRegularly( stDownload.Identifier, downloadStatusRunning )
      		
      		// Checks progress regularly
      		
      	// Paused download
      	case downloadStatusPaused   
      		
      		X is numeric = stDownload.Progress
      		if X <= 0 then X = 0
      		
      		// <Progress> of <Total size>
      		sStatus = "Paused"
      		
      		switch stDownload.StatusDetails
      			case downloadPausedWaitingToRetry 
      				sSizeInfo = "New attempt..."
      				
      			case downloadPausedWaitingForWifi  
      				sSizeInfo = "Waiting for WiFi network..."
      				
      			case downloadPausedWaitingForNetwork   
      				sSizeInfo = "Waiting for a network..."
      				
      			case downloadPausedUnknown  
      				sSizeInfo = LengthToString(X) + " on " + LengthToString(stDownload.FileSize) + " • " + stDownload.Source.Host
      				
      			other case
      				sSizeInfo = LengthToString(X) + " on " + LengthToString(stDownload.FileSize) + " • " + stDownload.Source.Host
      		end
      		
      		LOOP_DOWNLOADS[ N ].STC_FILE_INFO.Color = DarkOrange
      		
      		// Checks progress regularly
      		CheckDownloadRegularly( stDownload.Identifier, downloadStatusPaused )
      		
      	// Download completed
      	case downloadStatusSuccessful    
      		
      		// <Total size>
      		sSizeInfo = LengthToString(stDownload.FileSize) + " • " + stDownload.Source.Host
      		
      	// Download with errors
      	case downloadStatusFailed    
      		
      		sStatus = "Failed"
      		
      		switch stDownload.StatusDetails
      			case downloadErrorInsufficientSpace
      				sSizeInfo = "Not enough free space"
      				
      			case downloadErrorFileAlreadyExists 
      				sSizeInfo = "Invalid URL"
      				
      			case downloadErrorStorage  
      				sSizeInfo = "Unknown storage error"
      				
      			case downloadErrorUnknown 
      				sSizeInfo = "Unknown error"
      				
      			other case
      				sSizeInfo = "Unknown error"
      		end
      		
      		LOOP_DOWNLOADS[ N ].ATT_ERROR			= True
      		LOOP_DOWNLOADS[ N ].STC_FILE_INFO.Color	= DarkRed
      		
      end
      
      // Displays the name of the downloaded file
      LOOP_DOWNLOADS[ N ].STC_FILE_NAME					= URIGetInfo(stDownload.Destination, uriInfoName) + sTypeInfo.ExtractString(2) + ( [ " - " ] + sStatus )
      LOOP_DOWNLOADS[ N ].STC_FILE_INFO					= sSizeInfo
      
      return stDownload.FileSize
     type : 458752
   -
     name : CheckDownloadRegularly
     internal_properties : CAAAAAgAAAA5yvQThi6xoki6ht9swvLRW8h9MmVzEPZAFbUUmpc5oLaEN5YtU7bSgNGmxUtQgyiOkgMAz89365sE0opLJC5ghn89n7vZH50F4TGiaabmP8USB+QNXFPhRxiQriEWKP0sfJpCUXljKY1T/vEqjQ==
     procedure_id : 1409353274351532099
     type_code : 14
     code : |1-
      // Summary: Checks a download regularly
      // Syntax:
      //CheckDownloadRegularly (<ID> is string, <nStatus> is int)
      //
      // Parameters:
      //	ID (UNICODE string): Identifier of the download
      //	nStatus (integer): Current status of the download
      // Return value:
      // 	None
      //
      // Automatic procedure:
      // The procedure is executed manually, during a call in the code, with a timeout of 25 hundredths of a second
      // It will be run in a secure thread (without calling ThreadExecute), without using HFSQL
      // It will be repeated in loop, with 1 second between each call
      // 
      
      procedure CheckDownloadRegularly( local ID is string, local nState is int )
      
      stDownloadUpdated is DownloadInfo
      
      // Updates the details of the download
      stDownloadUpdated = DownloadGetInfo(ID)
      
      // If the download was pending or paused
      if nState in ( downloadStatusPending, downloadStatusPaused ) then
      	
      	// If its status has not changed
      	if stDownloadUpdated.Status = nState then
      		
      		// Ignore the rest, next passage
      		return
      	end		
      end
      
      // If the download was completed
      if nState in ( downloadStatusFailed, downloadStatusSuccessful ) then
      	
      	// Stops the timer
      	EndAutomatedProcedure()
      	return
      end
      
      // Update
      ExecuteMainThread( () => { 
      
      // Refreshes
      ShowDownload( stDownloadUpdated, False, True )
      
      // If the download is no longer in progress
      if stDownloadUpdated.Status <> downloadStatusRunning then
      	
      	// Refreshes the list
      	RefreshList()		
      end		
      
      } )
      
      // End of download
      if stDownloadUpdated.Status in ( downloadStatusFailed, downloadStatusSuccessful ) then
      	
      	// Stops the timer
      	EndAutomatedProcedure()
      end
     type : 458752
   -
     name : URLAdd
     procedure_id : 1409353274351597635
     type_code : 14
     code : |1-
      // Summary: Adds a URL for each input
      // Syntax:
      //URLAdd ( [<sDefaultURL> is string])
      //
      // Parameters:
      // 	sDefaultURL (UNICODE string - default value=""): Default URL
      // Return value:
      // 	None
      //
      procedure URLAdd( sDefaultURL is string = "" )
      
      // Opens the URL input popup
      WIN_POP_URL.OuvrePopupAsynchrone( ( sDefaultURL ), popCenteredWindow, procedure( ReturnedValue is string ) {
      	
      	// If the input has been validated
      	if ReturnedValue <> "" then
      		
      		// Program the download
      		DownloadElement( ReturnedValue )		
      	end	
      	
      } )
     type : 458752
